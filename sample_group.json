{
    "Group":   {
        "Name": "TesterGroup"
    },
    "Cores": [{
        "thingName": "ThingTesterCore",
        "SyncShadow": true,
        "useExistingThing": false
    }],
    "Devices": [
        {
            "thingName": "ThingTesterDevice",
            "SyncShadow": true,
            "useExistingThing": false
        }
    ],
    "Functions": [
        {
            "FunctionName": "gateway-dev-register_post",
            "FunctionAlias": "RegisterPost",
            "EncodingType": "json",
            "Executable": "handlers/register/main.post",
            "MemorySize": 50000,
            "Pinned": true,
            "Timeout": 5,
            "Environment": {
            }
        }
    ],
    "Resources": [
        {
            "Id": "defaultInterface",
            "Name": "defaultInterface",
            "ResourceDataContainer": {
                "LocalDeviceResourceData": {
                    "GroupOwnerSetting": {
                        "AutoAddGroupOwner": true
                    },
                    "SourcePath": "/dev/ttyAMA0"
                }
            }
        }
    ]
}
